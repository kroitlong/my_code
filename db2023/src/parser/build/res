CREATE TABLE t(bid bigint, sid int);
CREATE_TABLE
  t
  LIST
    COL_DEF
      bid
      TYPE_LEN
        BIGINT
        24
    COL_DEF
      sid
      TYPE_LEN
        INT
        4

