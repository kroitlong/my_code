create table warehouse (id int, name char(8));
CREATE_TABLE
  warehouse
  LIST
    COL_DEF
      id
      TYPE_LEN
        INT
        4
    COL_DEF
      name
      TYPE_LEN
        STRING
        8

create index warehouse (id);
CREATE_INDEX
  warehouse
  id

show index from warehouse;
SHOW INDEX
  warehouse

create index warehouse (id, name);
CREATE_INDEX
  warehouse
  id
  name

show index from warehouse;
SHOW INDEX
  warehouse

drop index warehouse (id);
DROP_INDEX
  warehouse
  id

drop index warehouse (id, name);
DROP_INDEX
  warehouse
  id
  name

show index from warehouse;
SHOW INDEX
  warehouse


